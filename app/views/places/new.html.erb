<%= form_for(@place) do |pl| %>

<%= pl.label :name, "Nome do Estabelecimento" %>
<%= pl.text_field :name %>

<%= pl.label :kind, "Tipo" %>
<%= pl.text_field :kind %>

<%= pl.label :description, "Descrição" %>
<%= pl.text_field :description %>

<%= pl.label :openinghours, "Horário de Funcionamento" %>
<%= pl.text_field :openinghours %>

<%= pl.label :address, "Endereço" %>
<%= pl.text_field :address %>


<%= pl.submit "Criar estabelecimento" %>

<% end %>
